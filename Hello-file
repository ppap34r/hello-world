public class ZzZcrazyClAsSnaMe_123{
public static void main(String[] args){
int[] OMGarray = {5,3,8,1,4};
sortNstuff(OMGarray);
for(int i=0;i<OMGarray.length;i++)
System.out.print(OMGarray[i]+" ");
}

public static void sortNstuff(int[] watArray){
for(int i=0;i<=watArray.length;i++){
for(int j=i+1;j<watArray.length;j++){
if(watArray[i] > watArray[j]){
int oops = watArray[i];
watArray[i] = watArray[j+1];
watArray[j] = oops;
}
}
}
}
}
